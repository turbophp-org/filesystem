{
  "name": "turbophp-org/filesystem",
  "description": "TurboPHP 框架文件系统库，提供统一的文件操作接口、目录管理和本地/远程存储支持，简化文件读写和管理。",
  "license": "Apache-2.0",
  "keywords": [
    "php",
    "turbophp",
    "framework",
    "filesystem",
    "storage",
    "file",
    "directory"
  ],
  "homepage": "https://turbophp.org",
  "support": {
    "issues": "https://github.com/turbophp-org/filesystem/issues",
    "source": "https://github.com/turbophp-org/filesystem",
    "docs": "https://turbophp.org/docs",
    "pull-request": "https://github.com/turbophp-org/filesystem/pulls"
  },
  "require": {
    "php": ">=8.1"
  },
  "autoload": {
    "psr-4": {
      "TurboPHP\\Filesystem\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0-dev"
    }
  }
}
